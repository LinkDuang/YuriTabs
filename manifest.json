{
  "manifest_version": 3,
  "name": "Yuri Tabs",
  "description": "将书签映射到「新标签页」，从书签栏中解放吧。",
  "action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.1.5",
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "permissions": [
    "bookmarks"
  ]
}