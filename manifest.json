{
  "manifest_version": 2,
  "name": "Yuri Tabs",
  "description": "serdes",
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "permissions": [
    "bookmarks",
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": [
      "script/sheet-url.js",
      "script/csv.js",
      "script/db-manager.js",
      "script/background.js"
    ],
    "persistent": false
  }
}